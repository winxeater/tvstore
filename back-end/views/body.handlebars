<section class="container">
        <div class="row py-5">
          <div class="col-2">
            <form action="/search-filter" class="search-form">
            <div class="row">
              <div class="col">
                <h1 class="card-title mt-0">Marca</h1>
                
                <div class="custom-control custom-checkbox mb-1">
                  <input class="custom-control-input" name="filter" value="samsung" id="samsungUnchecked1" type="checkbox"/>
                  <label class="custom-control-label" for="samsungUnchecked1">Samsung</label>
                </div>
                <div class="custom-control custom-checkbox mb-1">
                  <input class="custom-control-input" name="filter" value="lg" id="lgUnchecked1" type="checkbox"/>
                  <label class="custom-control-label" for="lgUnchecked1">LG</label>
                </div>
                <div class="custom-control custom-checkbox mb-1">
                  <input class="custom-control-input" name="filter" value="sony" id="sonyUnchecked1" type="checkbox"/>
                  <label class="custom-control-label" for="sonyUnchecked1">Sony</label>
                </div>
              </div>
              
            </div> <input type="submit" value="Filtrar">
            </form>
            <div class="row">
              <div class="col">
                <h1 class="card-title">Tipo</h1>
                <div class="custom-control custom-checkbox mb-1">
                  <input class="custom-control-input" id="lcdnchecked1" type="checkbox"/>
                  <label class="custom-control-label" for="lcdnchecked1">LCD</label>
                </div>
                <div class="custom-control custom-checkbox mb-1">
                  <input class="custom-control-input" id="lcdnchecked2" type="checkbox"/>
                  <label class="custom-control-label" for="lcdnchecked2">LCD</label>
                </div>
                <div class="custom-control custom-checkbox mb-1">
                  <input class="custom-control-input" id="lcdnchecked3" type="checkbox"/>
                  <label class="custom-control-label" for="lcdnchecked3">LCD</label>
                </div>
              </div>
            </div>
            <div class="row">
              <div class="col">
                <h1 class="card-title">Tamanho</h1>
                <div class="custom-control custom-checkbox mb-1">
                  <input class="custom-control-input" id="t55nchecked1" type="checkbox"/>
                  <label class="custom-control-label" for="t55nchecked1">55"</label>
                </div>
                <div class="custom-control custom-checkbox mb-1">
                  <input class="custom-control-input" id="t55nchecked2" type="checkbox"/>
                  <label class="custom-control-label" for="t55nchecked2">55"</label>
                </div>
                <div class="custom-control custom-checkbox mb-1">
                  <input class="custom-control-input" id="t55nchecked3" type="checkbox"/>
                  <label class="custom-control-label" for="t55nchecked3">55"</label>
                </div>
              </div>
            </div>
            <div class="row">
              <div class="col">
                <h1 class="card-title">Resolução</h1>
                <div class="custom-control custom-checkbox mb-1">
                  <input class="custom-control-input" id="fullhdnchecked1" type="checkbox"/>
                  <label class="custom-control-label" for="fullhdnchecked1">FullHD</label>
                </div>
                <div class="custom-control custom-checkbox mb-1">
                  <input class="custom-control-input" id="fullhdnchecked2" type="checkbox"/>
                  <label class="custom-control-label" for="fullhdnchecked2">FullHD</label>
                </div>
                <div class="custom-control custom-checkbox mb-1">
                  <input class="custom-control-input" id="fullhdnchecked3" type="checkbox"/>
                  <label class="custom-control-label" for="fullhdnchecked3">FullHD</label>
                </div>
              </div>
            </div>
            <div class="row">
              <div class="col">
                <h1 class="card-title">Voltagem</h1>
                <div class="custom-control custom-checkbox mb-1">
                  <input class="custom-control-input" id="voltagemnchecked1" type="checkbox"/>
                  <label class="custom-control-label" for="voltagemnchecked1">110v</label>
                </div>
                <div class="custom-control custom-checkbox mb-1">
                  <input class="custom-control-input" id="voltagemnchecked2" type="checkbox"/>
                  <label class="custom-control-label" for="voltagemnchecked2">110v</label>
                </div>
                <div class="custom-control custom-checkbox mb-1">
                  <input class="custom-control-input" id="voltagemnchecked3" type="checkbox"/>
                  <label class="custom-control-label" for="voltagemnchecked3">110v</label>
                </div>
              </div>
            </div>
            {{!-- </form> --}}
          </div>
          <div class="col-10">

            <div class="row">
              {{#each item}}
              <div class="col-4 mb-3">
                <div class="cz-product-item"><img class="cz-product-item__img mb-4" src="images/default-tv.png" alt=""/>
                  <h2>TV {{brand}}</h2>
                  <div class="cz-product-item__row"><span>Marca:</span><span>{{brand}}</span></div>
                  <div class="cz-product-item__row"><span>Tipo:</span><span>{{screen_type}}</span></div>
                  <div class="cz-product-item__row"><span>Tamanha:</span><span>{{screen_size}}</span></div>
                  <div class="cz-product-item__row"><span>Resolução:</span><span>{{resolution}}</span></div>
                  <div class="cz-product-item__row"><span>Voltagem:</span><span>{{voltage}}</span></div>
                  <h2 class="mt-3">Informações adicionais</h2>
                  <div class="cz-product-item__row"><span>Modelo:</span><span>USZ990055XD</span></div>
                  <div class="cz-product-item__row"><span>Saídas:</span><span>2 HDMI, 1 USB</span></div>
                  <div class="cz-product-item__row mb-4"><span>HDTV:</span><span>Sim</span></div>
                  <button class="btn btn-primary" type="button">Comprar</button>
                </div>
              </div>
              {{/each}}
            </div>
          </div>
        </div>
      </section>